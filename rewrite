## global variables

set $local https://localhost:3000;


# Example for https://medium.com/
domain glyph.medium.com {
  gzip on
      
  location ~ /css/e/sr/latin/e/ssr/latin/e/ssb/latin/m2.css {
    proxy_pass $local/Medium/m2.css;
  }

}
